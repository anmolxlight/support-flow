{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supportflow-supabase-url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supportflow-supabase-anon-key",
    "SUPABASE_SERVICE_ROLE_KEY": "@supportflow-supabase-service-key",
    "ELEVENLABS_API_KEY": "@supportflow-elevenlabs-api-key",
    "TWILIO_ACCOUNT_SID": "@supportflow-twilio-account-sid",
    "TWILIO_AUTH_TOKEN": "@supportflow-twilio-auth-token",
    "TWILIO_PHONE_NUMBER": "@supportflow-twilio-phone-number"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

